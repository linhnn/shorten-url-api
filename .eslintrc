{
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "consistent-return": 0,
    "global-require": 0,
    "no-empty-pattern": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-await-in-loop": 0,
    "func-names": 0,
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": true, "peerDependencies": true}],
    "prefer-rest-params": 0,
    "new-cap": 0,
  },
  "globals": {
    "Url": true,
    "Subscribe": true,
    "tiki": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
    "after": true,
    "afterEach": true,
    "it": true,
    "graphqlUrl": true,
    "graphqlAdminUrl": true,
    "should": true,
  }
}
